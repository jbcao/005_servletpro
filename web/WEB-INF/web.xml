<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="false">
    <context-param>
        <param-name>pageSize</param-name>
        <param-value>200</param-value>
    </context-param>
    <context-param>
        <param-name>pageNum</param-name>
        <param-value>3</param-value>
    </context-param>
    <servlet>
        <servlet-name>firstservlet</servlet-name>
        <servlet-class>com.practise.a_3interface.FirstServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>firstservlet</servlet-name>
        <url-pattern>/aaa</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>firstgenericservlet</servlet-name>
        <servlet-class>com.practise.a_3interface.MyGenericServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>firstgenericservlet</servlet-name>
        <url-pattern>/bbb</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>firsthttpservlet</servlet-name>
        <servlet-class>com.practise.a_3interface.MyHttpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>firsthttpservlet</servlet-name>
        <url-pattern>/ccc</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InitServlet</servlet-name>
        <servlet-class>com.practise.b_loadonstartup.InitServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>InitServlet</servlet-name>
        <url-pattern>/InitServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CounterInitServlet</servlet-name>
        <servlet-class>com.practise.l_servletcontext.counter.CounterInitServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>CounterInitServlet</servlet-name>
        <url-pattern>/CounterInitServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SecondServlet</servlet-name>
        <servlet-class>com.practise.b_loadonstartup.SecondServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>SecondServlet</servlet-name>
        <url-pattern>/SecondServlet</url-pattern>
    </servlet-mapping>
    <!--过滤器先注销了，防止对其他的有影响-->
    <!--<filter>-->
        <!--<filter-name>MyFilter</filter-name>-->
        <!--<filter-class>com.practise.k_filter.MyFilter</filter-class>-->
    <!--</filter>-->
    <!--<filter-mapping>-->
        <!--<filter-name>MyFilter</filter-name>-->
        <!--<url-pattern>/*</url-pattern>-->
    <!--</filter-mapping>-->

</web-app>
